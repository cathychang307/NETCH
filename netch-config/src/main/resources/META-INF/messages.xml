	<!-- Tping訊息格式 -->
    <message id="T001" direction="U" field-counts="6">
        <field index="0" type="X" length="7" desc="銀行代號" init="0040000"/>
        <field index="1" type="D" length="8" desc="銀行端系統日期：YYYYMMDD"/>
