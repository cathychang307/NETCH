        <transforms class="com.bot.cqs.gateway.service.message.ConnectMsg">
        	<rule field="bankId" index="0"/>
        	<rule field="trxDate" index="1"/>
        	<rule field="trxTime" index="2"/>
        	<rule field="trxCode" index="3"/>
        	<rule field="appropriative" index="4"/>
        	<rule field="sequenceNo" index="5"/>
        	<rule field="errCode" index="6"/>
        	<rule field="reserved" index="7"/>
        	<rule field="continueFlag" index="8"/>
        </transforms>
    </message>
    
	<!-- Start Services訊息格式 -->
    <message id="S001" direction="D" field-counts="6">
        <field index="0" type="X" length="7" desc="銀行代號" init="0040000"/>
        <field index="1" type="D" length="8" desc="銀行端系統日期：YYYYMMDD"/>
        <field index="2" type="T" length="6" desc="銀行端系統時間：HHmmss"/>
        <field index="3" type="X" length="4" desc="交易代碼" init="S001"/>
        <field index="4" type="X" length="20" desc="會員專用欄位"/>
        <field index="5" type="X" length="35" desc="保留欄位"/>
        <transforms class="com.bot.cqs.gateway.service.message.ServiceSessionMsg">
        	<rule field="bankId" index="0"/>
        	<rule field="trxDate" index="1"/>
        	<rule field="trxTime" index="2"/>
        	<rule field="trxCode" index="3"/>
        	<rule field="appropriative" index="4"/>
        	<rule field="reserved" index="5"/>
        </transforms>
    </message>
    
	<!-- Stop Services訊息格式 -->
    <message id="S005" direction="D" field-counts="6">
        <field index="0" type="X" length="7" desc="銀行代號" init="0040000"/>
        <field index="1" type="D" length="8" desc="銀行端系統日期：YYYYMMDD"/>
